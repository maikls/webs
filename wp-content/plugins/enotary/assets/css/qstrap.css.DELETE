height: 34px;
padding: 6px 12px;
font-size: 14px;
line-height: 1.42857143;
}

.qform label {width:100%; display:block; text-align:left;display: block;
max-width: 100%;
min-height: 34px;
padding: 10px 3px;
font-size: 14px;
line-height: 1.42857143;
margin:0px;
color: #665566;
} 


.qform input[type="text"], .qrow input[type="email"], .qrow input[type="password"], .qform textarea, .qform select, .qform .ed,
.qform input[type="number"], .qrow input[type="color"], .qrow input[type="url"], .qrow input[type="date"],
 .qrow input[type="time"], .qrow input[type="datetime"], .qrow input[type="tel"] , #body-form .ed-text {
display: block;
width: 100%;
padding: 6px 12px;
font-size: 14px;
