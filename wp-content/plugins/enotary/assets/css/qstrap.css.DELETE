.post-item .post-header a, .post-item .post-text a {color: #6388C6;}
.post-item .post-header a:hover, .post-item .post-text a:hover {color: #6898E8;}
.entry-meta span, .entry-meta a { font-size: 12px; color: gray; font-family: arial; font-weight:500; padding: 3px;}
.entry-meta a:hover { color: #64A9FF; }
