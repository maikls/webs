.fright {float: right;}
.fleft {float: left;}


.w-100 { width:100% !important; }
.w-50 { width:50% !important; }

.message-max {padding: 120px; display: block; margin: 0px; background-color: #f5f5f5; border-radius: 8px;}

.qform .required label {font-weight: bold;color: #665566;}
.qform .error { background-color: red; color: white; border: 2px solid white; border-radius: 6px;}

.qform input[type="text"], .qrow input[type="email"], .qrow input[type="password"], .qform textarea, .qform select,
.qform input[type="number"], .qrow input[type="color"], .qrow input[type="url"], .qrow input[type="date"], 
.qrow input[type="time"], .qrow input[type="datetime"], .qrow input[type="tel"], #body-form .ed-text
{width:100%;}

.qform input[type="button"]  {}
