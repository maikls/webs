.qrow {padding:0px; margin:0px;display:block;}
.qbox {padding:0px; margin:0px;display:block;}	
.qsection {padding:0px; margin:0px;display:block;}	
.qpadding {padding:10px;}
.qhalf {display:inline-block;float:left;width: 50%;}
.qcolumn {display:inline-block;float:left;}
